{
  "name": "vite-project-library",
  "private": false,
  "version": "0.0.15",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "main": "./dist/library-styleguide.umd.cjs",
  "module": "./dist.library-styleguide.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/library-styleguide.js",
      "require": "./dist/library-styleguide.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vionijannet/library-styleguide.git"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "path": "^0.12.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.2.4",
    "vue-tsc": "^2.0.6"
  }
}
